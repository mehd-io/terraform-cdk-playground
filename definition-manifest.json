{
	"variants": ["3", "3.8", "3.7", "3.6"],
	"build": {
		"latest": true,
		"rootDistro": "debian",
		"tags": [
			"python:${VERSION}-${VARIANT}"
		]
	},
	"dependencies": {
		"image": "python:${VARIANT}",
		"imageLink": "https://hub.docker.com/_/python",
		"debian": [
			"apt-utils", 
			"dialog", 
			"ca-certificates",
			"git",
			"iproute2",
			"procps",
			"curl",
			"wget",
			"less",
			"nano",
			"gnupg",
			"jq",
			"unzip",
			"lsb-release",
			"apt-transport-https",
			"libc6",
			"libgcc1",
			"libgssapi-krb5-2",
			"libicu63",
			"libssl1.1",
			"libstdc++6",
			"zlib1g",
			"sudo",
			"zsh"
		],
		"pipx": [
			"pylint",
			"flake8",
			"autopep8",
			"black",
			"yapf",
			"mypy",
			"pydocstyle",
			"pycodestyle",
			"bandit",
			"virtualenv",
			"pipx"
		],
		"manual": [
			{
				"Component": {
					"Type": "git",
					"git": {
						"Name": "Oh My Zsh!",
						"repositoryUrl": "https://github.com/robbyrussell/oh-my-zsh",
						"commitHash": "c130aadb6a66aa680a322c08d87ad773316f713d"
					}
				}
			}
		]
	}
}